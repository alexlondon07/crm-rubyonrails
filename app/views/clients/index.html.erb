<p id="notice"><%= notice %></p>

<h1>Listing Clients</h1>

<table>
  <thead>
    <tr>
      <th>Code sn</th>
      <th>Name</th>
      <th>Business name</th>
      <th>Nit</th>
      <th>Address</th>
      <th>City</th>
      <th>Contact name</th>
      <th>Contact phone1</th>
      <th>Contact phone2</th>
      <th>Contact email</th>
      <th>Enable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.code_sn %></td>
        <td><%= client.name %></td>
        <td><%= client.business_name %></td>
        <td><%= client.nit %></td>
        <td><%= client.address %></td>
        <td><%= client.city %></td>
        <td><%= client.contact_name %></td>
        <td><%= client.contact_phone1 %></td>
        <td><%= client.contact_phone2 %></td>
        <td><%= client.contact_email %></td>
        <td><%= client.enable %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
